{
  "private": true,
  "scripts": {
    "dev": "yarn generate && concurrently 'yarn:dev:*'",
    "generate": "nuxt generate && cp -R .output ./pocketbase",
    "dev:nuxt": "nuxt dev",
    "dev:pocketbase": "go run pocketbase/main.go serve",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && husky install",
    "build:prod": "nuxt generate && cp -R .output ./pocketbase && go build -o pocketnuxt pocketbase/main.go",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.6.6",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^6.6.5",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "nuxt": "^3.4.3",
    "nuxt-security": "^0.13.0",
    "prettier": "^2.8.8",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.28.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chart.js": "^4.2.1",
    "pocketbase": "^0.15.0"
  }
}
